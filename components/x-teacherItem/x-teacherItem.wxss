/* components/x-teacherItem/x-teacherItem.wxss */
.x-teacherItem{
  width: 100px;
  height:  130px;
  background-color: orange;
  border-radius: 10px;
}
.x-teacherItem image{
  width: 100px;
  height: 100px;
  border-radius: 50%;
  border: 5px dashed red;
  box-sizing: border-box;
}

.x-teacherItem text{
  width: 100%;
  display: block;
  text-align: center;
  color: white;
}